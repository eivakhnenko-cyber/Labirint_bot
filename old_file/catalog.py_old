async def process_catalog_deletion(update: Update, context: CallbackContext) -> None:
    """–û–±—Ä–∞–±–æ—Ç–∫–∞ —É–¥–∞–ª–µ–Ω–∏—è —Ç–æ–≤–∞—Ä–∞ –∏–∑ —Å–ø—Ä–∞–≤–æ—á–Ω–∏–∫–∞"""
    if 'deleting_from_catalog' not in context.user_data:
        return
    
    text = update.message.text.strip()
    process = context.user_data['deleting_from_catalog']
    step = process['step']
    user_id = update.effective_user.id
    
    if text == Buttons.CANCEL:
        del context.user_data['deleting_from_catalog']
        await update.message.reply_text(
            "‚ùå –£–¥–∞–ª–µ–Ω–∏–µ –æ—Ç–º–µ–Ω–µ–Ω–æ.",
            reply_markup=await get_catalog_keyboard(user_id)
        )
        return
    
    if step == 'category':
        if not text:
            await update.message.reply_text("–í–≤–µ–¥–∏—Ç–µ –∫–∞—Ç–µ–≥–æ—Ä–∏—é —Ç–æ–≤–∞—Ä–∞:")
            return
        
        # –ü—Ä–æ–≤–µ—Ä—è–µ–º —Å—É—â–µ—Å—Ç–≤–æ–≤–∞–Ω–∏–µ –∫–∞—Ç–µ–≥–æ—Ä–∏–∏
        if not CatalogRepository.check_category_exists(text):
            await update.message.reply_text(
                f"‚ùå –í –∫–∞—Ç–µ–≥–æ—Ä–∏–∏ '{text}' –Ω–µ—Ç –∞–∫—Ç–∏–≤–Ω—ã—Ö —Ç–æ–≤–∞—Ä–æ–≤.\n"
                f"–í–≤–µ–¥–∏—Ç–µ –¥—Ä—É–≥—É—é –∫–∞—Ç–µ–≥–æ—Ä–∏—é:",
                reply_markup=get_cancel_keyboard()
            )
            return
        
        process['data']['category'] = text
        process['step'] = 'select_product'
        
        # –ü–æ–ª—É—á–∞–µ–º —Ç–æ–≤–∞—Ä—ã –∏–∑ –≤—ã–±—Ä–∞–Ω–Ω–æ–π –∫–∞—Ç–µ–≥–æ—Ä–∏–∏
        products = CatalogRepository.get_category_products(text)
        
        if not products:
            await update.message.reply_text(
                f"‚ùå –í –∫–∞—Ç–µ–≥–æ—Ä–∏–∏ '{text}' –Ω–µ—Ç —Ç–æ–≤–∞—Ä–æ–≤ –¥–ª—è —É–¥–∞–ª–µ–Ω–∏—è.",
                reply_markup=await get_catalog_keyboard(user_id)
            )
            del context.user_data['deleting_from_catalog']
            return
        
        process['data']['products'] = products
        
        # –§–æ—Ä–º–∏—Ä—É–µ–º —Å–ø–∏—Å–æ–∫ —Ç–æ–≤–∞—Ä–æ–≤ –¥–ª—è –≤—ã–±–æ—Ä–∞
        products_text = ""
        buttons = []
        
        for idx, product in enumerate(products, 1):
            products_text += f"\n{idx}. üè∑Ô∏è *{product['name']}*\n"
            products_text += f"   üìè {product['unit']} | üî¢ {product['default_quantity']}\n"
            if product['description']:
                products_text += f"   üìù {product['description']}\n"
            products_text += f"   üÜî ID: {product['product_id']}\n"
            
            # –°–æ–∑–¥–∞–µ–º –∫–Ω–æ–ø–∫–∏ –¥–ª—è –±—ã—Å—Ç—Ä–æ–≥–æ –≤—ã–±–æ—Ä–∞
            btn_text = f"{idx}. {product['name'][:25]}"
            if len(btn_text) > 30:
                btn_text = f"{idx}. {product['name'][:20]}..."
            buttons.append([btn_text])
        
        # –î–æ–±–∞–≤–ª—è–µ–º –∫–Ω–æ–ø–∫—É –¥–ª—è —É–¥–∞–ª–µ–Ω–∏—è –≤—Å–µ–π –∫–∞—Ç–µ–≥–æ—Ä–∏–∏
        buttons.append(["üóëÔ∏è –£–¥–∞–ª–∏—Ç—å –í–°–ï —Ç–æ–≤–∞—Ä—ã –∫–∞—Ç–µ–≥–æ—Ä–∏–∏"])
        buttons.append(["‚ùå –û—Ç–º–µ–Ω–∞"])
        
        keyboard = ReplyKeyboardMarkup(buttons, resize_keyboard=True, one_time_keyboard=True)
        
        await update.message.reply_text(
            f"üìã *–¢–æ–≤–∞—Ä—ã –≤ –∫–∞—Ç–µ–≥–æ—Ä–∏–∏: {text}*\n\n"
            f"–í—ã–±–µ—Ä–∏—Ç–µ —Ç–æ–≤–∞—Ä –¥–ª—è —É–¥–∞–ª–µ–Ω–∏—è:{products_text}\n\n"
            f"*–ò–ª–∏ —É–¥–∞–ª–∏—Ç–µ –≤—Å–µ —Ç–æ–≤–∞—Ä—ã –∫–∞—Ç–µ–≥–æ—Ä–∏–∏:*",
            reply_markup=keyboard,
            parse_mode='Markdown'
        )
    
    elif step == 'select_product':
        if text == "üóëÔ∏è –£–¥–∞–ª–∏—Ç—å –í–°–ï —Ç–æ–≤–∞—Ä—ã –∫–∞—Ç–µ–≥–æ—Ä–∏–∏":
            process['step'] = 'confirm_all'
            
            await update.message.reply_text(
                f"‚ö†Ô∏è *–í–ù–ò–ú–ê–ù–ò–ï!*\n\n"
                f"–í—ã —Å–æ–±–∏—Ä–∞–µ—Ç–µ—Å—å —É–¥–∞–ª–∏—Ç—å *–í–°–ï —Ç–æ–≤–∞—Ä—ã* –∏–∑ –∫–∞—Ç–µ–≥–æ—Ä–∏–∏: *{process['data']['category']}*\n\n"
                f"–ö–æ–ª–∏—á–µ—Å—Ç–≤–æ —Ç–æ–≤–∞—Ä–æ–≤ –¥–ª—è —É–¥–∞–ª–µ–Ω–∏—è: {len(process['data']['products'])}\n\n"
                f"–≠—Ç–æ –¥–µ–π—Å—Ç–≤–∏–µ *–ù–ï–õ–¨–ó–Ø* –æ—Ç–º–µ–Ω–∏—Ç—å! –í—Å–µ –¥–∞–Ω–Ω—ã–µ –±—É–¥—É—Ç —É–¥–∞–ª–µ–Ω—ã –±–µ–∑ –≤–æ–∑–º–æ–∂–Ω–æ—Å—Ç–∏ –≤–æ—Å—Å—Ç–∞–Ω–æ–≤–ª–µ–Ω–∏—è.\n\n"
                f"–ü–æ–¥—Ç–≤–µ—Ä–¥–∏—Ç–µ —É–¥–∞–ª–µ–Ω–∏–µ:",
                reply_markup=ReplyKeyboardMarkup(
                    [["‚úÖ –î–∞, —É–¥–∞–ª–∏—Ç—å –í–°–ï", "‚ùå –ù–µ—Ç, –æ—Ç–º–µ–Ω–∏—Ç—å"]],
                    resize_keyboard=True,
                    one_time_keyboard=True
                ),
                parse_mode='Markdown'
            )
        else:
            # –û–±—Ä–∞–±–æ—Ç–∫–∞ –≤—ã–±–æ—Ä–∞ –∫–æ–Ω–∫—Ä–µ—Ç–Ω–æ–≥–æ —Ç–æ–≤–∞—Ä–∞
            try:
                # –ü—ã—Ç–∞–µ–º—Å—è –æ–ø—Ä–µ–¥–µ–ª–∏—Ç—å –Ω–æ–º–µ—Ä —Ç–æ–≤–∞—Ä–∞
                selection = None
                if text.startswith(tuple(str(i) for i in range(1, 10))):
                    idx_part = text.split('.')[0]
                    if idx_part.isdigit():
                        idx = int(idx_part)
                        if 1 <= idx <= len(process['data']['products']):
                            selection = process['data']['products'][idx - 1]
                
                # –ï—Å–ª–∏ –Ω–µ —É–¥–∞–ª–æ—Å—å –ø–æ –Ω–æ–º–µ—Ä—É, –ø—Ä–æ–±—É–µ–º –Ω–∞–π—Ç–∏ –ø–æ –Ω–∞–∑–≤–∞–Ω–∏—é
                if selection is None:
                    for product in process['data']['products']:
                        if product['name'].lower() in text.lower():
                            selection = product
                            break
                
                if selection is None:
                    await update.message.reply_text(
                        "‚ùå –¢–æ–≤–∞—Ä –Ω–µ –Ω–∞–π–¥–µ–Ω. –í—ã–±–µ—Ä–∏—Ç–µ —Ç–æ–≤–∞—Ä –∏–∑ —Å–ø–∏—Å–∫–∞:",
                        reply_markup=ReplyKeyboardMarkup(
                            [[f"{i+1}. {p['name'][:25]}"] for i, p in enumerate(process['data']['products'])] +
                            [["üóëÔ∏è –£–¥–∞–ª–∏—Ç—å –í–°–ï —Ç–æ–≤–∞—Ä—ã –∫–∞—Ç–µ–≥–æ—Ä–∏–∏", "‚ùå –û—Ç–º–µ–Ω–∞"]],
                            resize_keyboard=True,
                            one_time_keyboard=True
                        )
                    )
                    return
                
                process['data']['selected_product'] = selection
                process['step'] = 'confirm_single'
                
                await update.message.reply_text(
                    f"‚ùì *–ü–æ–¥—Ç–≤–µ—Ä–¥–∏—Ç–µ —É–¥–∞–ª–µ–Ω–∏–µ —Ç–æ–≤–∞—Ä–∞:*\n\n"
                    f"üè∑Ô∏è *–ù–∞–∑–≤–∞–Ω–∏–µ:* {selection['name']}\n"
                    f"üìÅ *–ö–∞—Ç–µ–≥–æ—Ä–∏—è:* {process['data']['category']}\n"
                    f"üìè *–ï–¥–∏–Ω–∏—Ü–∞ –∏–∑–º–µ—Ä–µ–Ω–∏—è:* {selection['unit']}\n"
                    f"üî¢ *–°—Ç–∞–Ω–¥–∞—Ä—Ç–Ω–æ–µ –∫–æ–ª–∏—á–µ—Å—Ç–≤–æ:* {selection['default_quantity']}\n"
                    f"üÜî *ID —Ç–æ–≤–∞—Ä–∞:* {selection['product_id']}\n\n"
                    f"–≠—Ç–æ –¥–µ–π—Å—Ç–≤–∏–µ –º–æ–∂–Ω–æ –æ—Ç–º–µ–Ω–∏—Ç—å, —É—Å—Ç–∞–Ω–æ–≤–∏–≤ —Ç–æ–≤–∞—Ä –∫–∞–∫ –Ω–µ–∞–∫—Ç–∏–≤–Ω—ã–π.\n"
                    f"–£–¥–∞–ª–∏—Ç—å —Ç–æ–≤–∞—Ä?",
                    reply_markup=ReplyKeyboardMarkup(
                        [["‚úÖ –î–∞, —É–¥–∞–ª–∏—Ç—å", "üö´ –ù–µ—Ç, –æ—Ç–º–µ–Ω–∏—Ç—å"]],
                        resize_keyboard=True,
                        one_time_keyboard=True
                    ),
                    parse_mode='Markdown'
                )
                
            except Exception as e:
                logger.error(f"–û—à–∏–±–∫–∞ –æ–±—Ä–∞–±–æ—Ç–∫–∏ –≤—ã–±–æ—Ä–∞ —Ç–æ–≤–∞—Ä–∞: {e}")
                await update.message.reply_text(
                    "‚ùå –û—à–∏–±–∫–∞ –ø—Ä–∏ –æ–±—Ä–∞–±–æ—Ç–∫–µ –≤—ã–±–æ—Ä–∞. –ü–æ–ø—Ä–æ–±—É–π—Ç–µ —Å–Ω–æ–≤–∞:",
                    reply_markup=await get_catalog_keyboard(user_id)
                )
                del context.user_data['deleting_from_catalog']
    
    elif step == 'confirm_single':
        if text == "‚úÖ –î–∞, —É–¥–∞–ª–∏—Ç—å":
            await delete_single_product(update, context, process['data'])
        else:
            del context.user_data['deleting_from_catalog']
            await update.message.reply_text(
                "‚ùå –£–¥–∞–ª–µ–Ω–∏–µ –æ—Ç–º–µ–Ω–µ–Ω–æ.",
                reply_markup=await get_catalog_keyboard(user_id)
            )
    
    elif step == 'confirm_all':
        if text == "‚úÖ –î–∞, —É–¥–∞–ª–∏—Ç—å –í–°–ï":
            await delete_all_category_products(update, context, process['data'])
        else:
            del context.user_data['deleting_from_catalog']
            await update.message.reply_text(
                "‚ùå –£–¥–∞–ª–µ–Ω–∏–µ –æ—Ç–º–µ–Ω–µ–Ω–æ.",
                reply_markup=await get_catalog_keyboard(user_id)
            )

async def delete_single_product(update: Update, context: CallbackContext, data: dict) -> None:
    """–£–¥–∞–ª–µ–Ω–∏–µ –æ–¥–Ω–æ–≥–æ —Ç–æ–≤–∞—Ä–∞ –∏–∑ —Å–ø—Ä–∞–≤–æ—á–Ω–∏–∫–∞"""
    user_id = update.effective_user.id
    product = data['selected_product']
    
    success = CatalogRepository.soft_delete_product(product['product_id'])
    
    if success:
        del context.user_data['deleting_from_catalog']
        
        await update.message.reply_text(
            f"‚úÖ *–¢–æ–≤–∞—Ä —É—Å–ø–µ—à–Ω–æ —É–¥–∞–ª–µ–Ω!*\n\n"
            f"üè∑Ô∏è *–ù–∞–∑–≤–∞–Ω–∏–µ:* {product['name']}\n"
            f"üìÅ *–ö–∞—Ç–µ–≥–æ—Ä–∏—è:* {data['category']}\n"
            f"üÜî *ID —Ç–æ–≤–∞—Ä–∞:* {product['product_id']}\n\n"
            f"–¢–æ–≤–∞—Ä –ø–æ–º–µ—á–µ–Ω –∫–∞–∫ –Ω–µ–∞–∫—Ç–∏–≤–Ω—ã–π –∏ –±–æ–ª—å—à–µ –Ω–µ –±—É–¥–µ—Ç –æ—Ç–æ–±—Ä–∞–∂–∞—Ç—å—Å—è –≤ —Å–ø—Ä–∞–≤–æ—á–Ω–∏–∫–µ.",
            reply_markup=await get_catalog_keyboard(user_id),
            parse_mode='Markdown'
        )
    else:
        await update.message.reply_text(
            "‚ùå –û—à–∏–±–∫–∞ –ø—Ä–∏ —É–¥–∞–ª–µ–Ω–∏–∏ —Ç–æ–≤–∞—Ä–∞.",
            reply_markup=await get_catalog_keyboard(user_id)
        )

async def delete_all_category_products(update: Update, context: CallbackContext, data: dict) -> None:
    """–£–¥–∞–ª–µ–Ω–∏–µ –≤—Å–µ—Ö —Ç–æ–≤–∞—Ä–æ–≤ –∫–∞—Ç–µ–≥–æ—Ä–∏–∏"""
    user_id = update.effective_user.id
    category = data['category']
    
    deleted_count = CatalogRepository.soft_delete_category_products(category)
    
    if deleted_count > 0:
        del context.user_data['deleting_from_catalog']
        
        await update.message.reply_text(
            f"‚úÖ *–ö–∞—Ç–µ–≥–æ—Ä–∏—è –ø–æ–ª–Ω–æ—Å—Ç—å—é –æ—á–∏—â–µ–Ω–∞!*\n\n"
            f"üìÅ *–ö–∞—Ç–µ–≥–æ—Ä–∏—è:* {category}\n"
            f"üóëÔ∏è *–£–¥–∞–ª–µ–Ω–æ —Ç–æ–≤–∞—Ä–æ–≤:* {deleted_count}\n\n"
