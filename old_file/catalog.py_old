    if not await role_manager.has_permission(user_id, Permission.MANAGE_INVENTORY):
        await update.message.reply_text(
            "‚ùå –£ –≤–∞—Å –Ω–µ—Ç –ø—Ä–∞–≤ –¥–ª—è —É–ø—Ä–∞–≤–ª–µ–Ω–∏—è —Å–ø—Ä–∞–≤–æ—á–Ω–∏–∫–æ–º —Ç–æ–≤–∞—Ä–æ–≤.",
            reply_markup=await get_main_keyboard(user_id)
        )
        return
    
    await update.message.reply_text(
        "üìã *–£–ø—Ä–∞–≤–ª–µ–Ω–∏–µ —Å–ø—Ä–∞–≤–æ—á–Ω–∏–∫–æ–º —Ç–æ–≤–∞—Ä–æ–≤*\n\n"
        "–í—ã–±–µ—Ä–∏—Ç–µ –¥–µ–π—Å—Ç–≤–∏–µ:",
        reply_markup=await get_catalog_keyboard(user_id),
        parse_mode='Markdown'
    )

